{
  "name": "keep-it-batch-bot",
  "version": "0.0.1",
  "description": "Keep it batch jobs (site/note/image analysis)",
  "private": true,
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "prebuild:new": "rimraf dist-new",
    "build:new": "tsc -p tsconfig.json --outDir dist-new && tsc-alias -p tsconfig.json --dir dist-new",
    "deploy": "node scripts/deploy.js",
    "start": "yarn build && pm2 start ecosystem.config.js",
    "test": "jest --config jest.config.js",
    "lint": "eslint --quiet \"**/*.ts\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^16.0.0",
    "lodash": "^4.17.21",
    "openai": "^4.103.0",
    "pg": "^8.11.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/jest": "29.5.8",
    "@types/lodash": "^4.17.0",
    "@types/node": "16.11.62",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "dotenv-cli": "^7.0.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.5.0",
    "jest": "29.7.0",
    "prettier": "2.7.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  }
}
